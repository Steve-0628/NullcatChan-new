{
	"jsc": {
		"parser": {
			"syntax": "typescript",
			"tsx": false,
			"decorators": true,
			"dynamicImport": false
		},
		"target": "esnext",
		"keepClassNames": true,
		"transform": {
			"legacyDecorator": false,
			"decoratorMetadata": false
		},
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"]
		}
	},
	"module": {
		"type": "commonjs",
		"noInterop": false
	}
}
